{
  "configurations": [
    {
      "name": "Linux",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/Build/generated/pico_base",
        "${workspaceFolder}/Build/generated",
        "${workspaceFolder}/../../pico-sdk/src/**",
        "${workspaceFolder}/../../pico-sdk/lib/**"
      ],
      "defines": [
        "PICO_ON_DEVICE=1"
